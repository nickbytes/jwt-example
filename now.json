{
  "version": 2,
  "name": "jwt-example",
  "builds": [
    { "src": "./api/v1/auth.js", "use": "@now/node" },
    { "src": "./api/v1/users.js", "use": "@now/node" }
  ],
  "routes": [
    {
      "src": "/api/v1/auth",
      "dest": "/api/v1/auth.js"
    },
    {
      "src": "/api/v1/users",
      "dest": "/api/v1/users.js"
    }
  ]
}
